<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.react.pojo.TestMapper" >

    <!-- 插入一条图片信息数据 -->
  <insert id="addTest" parameterType="com.react.entity.Test" useGeneratedKeys="true" keyProperty="id" >
      insert into
        test
      values
        (#{id},#{name},#{dep},#{birthday},#{pic_name})
  </insert>

</mapper>