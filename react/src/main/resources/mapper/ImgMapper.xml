<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.react.pojo.ImgMapper" >

    <!-- 插入一条图片信息数据 -->
  <insert id="addImg" parameterType="com.react.entity.Img" >
      insert into
        img
      values
        (null,#{imgName},#{introduce},#{imgPath},#{comment})
  </insert>

    <select id="selectAllImg" resultType="com.react.entity.Img">
        select
          *
        from
          img
    </select>

</mapper>